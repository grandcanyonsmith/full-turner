{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-API-URL",
          "value": "https://i5of4xggwlglu3f477pzq24o7u0jrwms.lambda-url.us-west-2.on.aws"
        }
      ]
    }
  ]
}
