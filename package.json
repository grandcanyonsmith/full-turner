{
  "name": "full-turner",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "seed-template": "node scripts/seedTemplate.js",
    "test-workflow": "node scripts/testWorkflow.js",
    "test:e2e": "node scripts/test-e2e.js",
    "verify:template": "node scripts/verify-template.js",
    "check:run": "node scripts/check-run.js",
    "get:output": "node scripts/get-output.js",
    "setup:api-key": "node scripts/setup-api-key.js",
    "invoke:batch": "sam local invoke ProcessImagesBatchFunction --template infrastructure/template.yaml --event events/processImagesBatch.json",
    "invoke:aggregate": "sam local invoke AggregateImageResultsFunction --template infrastructure/template.yaml --event events/aggregateResults.json",
    "local:batch": "node scripts/invoke-local.js ProcessImagesBatchFunction events/processImagesBatch.json",
    "local:aggregate": "node scripts/invoke-local.js AggregateImageResultsFunction events/aggregateResults.json",
    "local:init": "node scripts/invoke-local.js InitializeRunFunction events/initializeRun.json",
    "local:load": "node scripts/invoke-local.js LoadTemplateFunction events/loadTemplate.json",
    "local:process-image": "node scripts/invoke-local.js ProcessImageFunction events/processImage.json",
    "local:execute": "node scripts/invoke-local.js ExecuteAgentFunction events/executeAgent.json",
    "local:save": "node scripts/invoke-local.js SaveOutputFunction events/saveOutput.json",
    "local:failure": "node scripts/invoke-local.js HandleFailureFunction events/handleFailure.json",
    "test:boxing-aws": "node scripts/test-boxing-course-aws.js"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.700.0",
    "@aws-sdk/client-dynamodb": "^3.700.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/client-secrets-manager": "^3.700.0",
    "@aws-sdk/client-sfn": "^3.700.0",
    "@aws-sdk/lib-dynamodb": "^3.700.0",
    "@openai/agents": "^0.1.0",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "esbuild": "^0.25.12"
  }
}
